{
	"compilerOptions": {
		"target": "ES2021",
		"module": "ESNext",
		"lib": [
			"ES2021",
			"DOM"
		],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"outDir": "../../.moon/cache/types/tests/backend-api",
		"baseUrl": ".",
		"paths": {
			"@ryot/graphql/*": [
				"../../libs/graphql/src/*"
			],
			"@ryot/graphql": [
				"../../libs/graphql/src/index.ts"
			]
		},
		"types": [
			"vitest/globals",
			"node"
		]
	},
	"include": [
		"src/**/*.ts",
		"vitest.config.ts"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}
