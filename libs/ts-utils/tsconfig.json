{
	"extends": "../../tsconfig.options.json",
	"references": [
		{
			"path": "../../libs/generated"
		},
		{
			"path": "../generated"
		}
	],
	"compilerOptions": {
		"strict": true,
		"module": "esnext",
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "node",
		"preserveConstEnums": true,
		"verbatimModuleSyntax": false,
		"allowSyntheticDefaultImports": true,
		"outDir": "../../.moon/cache/types/libs/ts-utils",
		"paths": {
			"@ryot/generated/*": ["../generated/src/*"]
		}
	}
}
