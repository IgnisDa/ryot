---
interface Props {
	class?: string;
}

const baseClass = "h-10 md:h-12 md:w-12 w-10";

const { class: className = baseClass } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  fill="none"
  viewBox="0 0 24 24"
  stroke-width="1.5"
  stroke="currentColor"
  class={className}
>
  <path stroke-linecap="round" stroke-linejoin="round" d="m4.5 12.75 6 6 9-13.5"
  ></path>
</svg>
